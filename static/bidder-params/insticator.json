
{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Insticator Adapter Params",
    "description": "A schema which validates params accepted by Insticator",
  
    "type": "object",
    "properties": {
      "productId": {
        "type": "string",
        "description": "Product type"
      },
      "adUnitId": {
        "type": "string",
        "description": "Ad Unit Id"
      },
      "zoneId": {
        "type": "string",
        "description": "Zone Id"
      }
    },
    "required": ["adUnitId"]
  }